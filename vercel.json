{
  "version": 2,
  "builds": [
    {
      "src": "dist/e-commerce-app/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/e-commerce-app/server/server.mjs"
    }
  ],
  "functions": {
    "dist/e-commerce-app/server/server.mjs": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
} 